{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/app\\.html",
      "dest": "/api/index.js"
    },
    {
      "src": "/app\\.js",
      "dest": "/api/index.js"
    },
    {
      "src": "^/$",
      "dest": "/api/index.js"
    },
    {
      "src": "/login\\.html",
      "dest": "/public/login.html"
    },
    {
      "src": "/ReBillion_Onboarding_Flow\\.html",
      "dest": "/public/ReBillion_Onboarding_Flow.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
